get_LjungBox_pvalues <- function(resd, max.lag, p, q){
df <- p + q
pv <- c()
for(i in df+1: max.lag){
p <- Box.test(resd, i, type = "Ljung-Box", fitdf = p+q )
pv = append(pv, p)
}
return(pv)
}
