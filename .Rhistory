# Extracts Ljung - Box statistic p -values for an ARMA(p,d,q)
# NOTE: df = p + q
LJB_pvalues <- function(Model, max.lag, df){
resd = Model[["fit"]][["residuals"]]
pv <- c()
for(i in df+1: max.lag){
p <- Box.test(resd, i, type = "Ljung-Box", fitdf = df)$p.value
pv = append(pv, p)
}
return(pv)
}
